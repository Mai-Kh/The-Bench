<?xml version="1.0" encoding="utf-8"?>
<!ELEMENT TheBench (Artist, Art, Event, Apply)>

<!ELEMENT Artist (name+, bio+, medium+, showcase?, contact?)>
  <!ELEMENT name>
  <!ELEMENT bio>
  <!ELEMENT medium>
  <!ELEMENT showcase>
  <!ELEMENT contact (email*, phone*)>
    <!ELEMENT email>
    <!ELEMENT phone>

<!ELEMENT Art (artist+, title+, subject?, date?, provenance?, location?, technique?, medium?)>
  <!ELEMENT artist>
  <!ELEMENT title>
  <!ELEMENT subject>
  <!ELEMENT date>
  <!ELEMENT provenance>
  <!ELEMENT location>
