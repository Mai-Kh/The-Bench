  <!ELEMENT medium>

<!ELEMENT Event (location+, date+, time+, artists?, misc?)>
  <!ELEMENT location>
  <!ELEMENT date>
  <!ELEMENT time>
  <!ELEMENT artists>
  <!ELEMENT misc>

<!ELEMENT Apply (name+, contact+, medium?)>
